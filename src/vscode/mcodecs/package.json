{
    "name": "mcodecs",
    "displayName": "mcodecs",
    "description": "mcodecs",
    "version": "0.0.1",
    "publisher": "maptz",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*",
        "onCommand:extension.sortClass"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sortClass",
                "title": "Sort Class"
            }
        ],
        "keybindings": {
            "command": "extension.sortClass",
            "key": "ctrl+m ctrl+p"
        },
        "configuration": {
            "title": "mcodecs configuration",
            "type": "object",
            "properties": {
                "maptz.mcodecs": {
                    "type": "object",
                    "description": "Settings used for maptz sort.",
                    "default": {}
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/adm-zip": "^0.4.32",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "@types/request": "^2.48.1",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "adm-zip": "^0.4.13",
        "request": "^2.88.0"
    }
}
