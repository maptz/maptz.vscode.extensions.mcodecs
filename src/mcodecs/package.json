{
    "name": "mcodecs",
    "displayName": "mcodecs",
    "description": "mcodecs",
    "version": "0.0.1",
    "publisher": "maptz",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sortClass"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sortClass",
                "title": "Sort Class"
            }
        ],
        "keybindings":{
            "command": "extension.sortClass",
            "key": "ctrl+m ctrl+p"
        }
    },
    
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
